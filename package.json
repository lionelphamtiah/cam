{
  "name": "kintone-plugin-template",
  "version": "0.1.0",
  "scripts": {
    "start": "node scripts/npm-start.js",
    "upload": "kintone-plugin-uploader dist/plugin.zip",
    "develop": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "upload:watch": "kintone-plugin-uploader dist/plugin.zip --watch --waiting-dialog-ms 3000",
    "update": "ncu -u",
    "generate:ppk": "node scripts/generate-ppk.js"
  },
  "dependencies": {
    "@kintone/plugin-uploader": "^7.1.2",
    "@kintone/webpack-plugin-kintone-plugin": "^6.0.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-google": "^0.14.0",
    "html5-qrcode": "^2.2.1",
    "node-rsa": "^1.1.1",
    "npm-check-updates": "^16.2.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
